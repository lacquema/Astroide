outputs: out
systems: x86_64-linux i686-linux
dependencies: emacs-helm@3.8.0
location: ]8;;file://f-dahu/gnu/store/a6h2f77ydqlgs6pgfxl0rpw6vzxxn142-guix-module-union/share/guile/site/3.0/gnu/packages/emacs-xyz.scm\gnu/packages/emacs-xyz.scm:20559:4]8;;\
